<%- include("01.header.ejs") %>

    <div class="row bg-white ">
        <h3 style="font-family:  sans-serif;" class="text-secondary"></h3>
        <h4 class="text-secondary mb-2">Resume Details Page</h4>
        <div class="col-md-12 " style="border-radius: 20px;box-shadow: inset 0px 0px 10px black;">
            <form action="/admin/update_technical_part_resume" method="post" enctype="multipart/form-data">


                <div class="row p-2">

                   
                    <div class=" col">
                        <label class="mb-2 pt-2 text-dark">Technical Skill Name</label>
                        <input type="text" class="form-control border border-danger" name="id" value="<%= resume_technical_part.id %>" hidden>

                        <input type="text" class="form-control border border-danger" name="resume_technical_skill_tech"
                        value="<%= resume_technical_part.resume_technical_skill_tech %>">

                    </div>
                    <div class="col">
                        <label class="mb-2 pt-2 text-dark">Technical Skill (percentage)</label>
                        <input type="text" class="form-control border border-danger" name="resume_tech_prog_bar_per"
                        value="<%= resume_technical_part.resume_tech_prog_bar_per %>">
                    </div>


                 

                    <div class="row p-4">
                        <div class="col text-center">

                            <button class="btn btn-success">update resume</button>
                        </div>
                    </div>

                </div>
            </form>
        </div>
    </div>

    <%- include("02.footer.ejs") %>