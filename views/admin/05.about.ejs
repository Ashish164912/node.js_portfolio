<%- include("01.header.ejs") %>

    <div class="row bg-white ">
        <h3 style="font-family:  sans-serif;" class="text-secondary">Home Page</h3>
     
        <div class="col-md-12 " style="border-radius: 20px;box-shadow: inset 0px 0px 10px black;">
            <form action="/admin/save_about_page" method="post" enctype="multipart/form-data">
                <div class="row p-2">
                    <div class="col">
                        <label class="mb-2 pt-2 text-dark">Email</label>
                        <input type="email" class="form-control border border-danger" name="About_Email"
                            value="<%= Aboutdata.About_Email %>">
                    </div>
                    <div class=" col">
                        <label class="mb-2 pt-2 text-dark">Mobile No</label>
                        <input type="number" class="form-control border border-danger" name="About_Mobile"
                            value="<%= Aboutdata.About_Mobile %>">
                    </div>
                    <div class="col">
                        <label class="mb-2 pt-2 text-dark">Address</label>
                        <input type="text" class="form-control border border-danger" name="About_Address"
                            value="<%= Aboutdata.About_Address %>">
                    </div>

                    <div class="col">
                        <label class="mb-2 pt-2 text-dark">Header</label>
                        <input type="text" class="form-control border border-danger" name="About_Header"
                            value="<%= Aboutdata.About_Header %>">
                    </div>

                </div>

                <div class="row p-2">

                    <div class=" col">
                        <label class="mb-2 text-dark">About Desript</label>

                        <textarea class="form-control border border-danger" name="About_Desript"
                            value="<%= Aboutdata.About_Desript %>">
                            <%= Aboutdata.About_Desript %>
                        </textarea>
                    </div>
                </div>

                <div class="row p-2">


                    <div class="col">
                        <label class="mb-2 text-dark">project complete</label>
                        <input type="number" class="form-control border border-danger" name="About_project_complete"
                            value="<%= Aboutdata.About_project_complete %>">
                    </div>
                    <div class="col">
                        <label class="mb-2 text-dark">year Experience</label>
                        <input type="number" class="form-control border border-danger" name="About_year_Exp"
                            value="<%= Aboutdata.About_year_Exp %>">
                    </div>
                    <div class="col">
                        <label class="mb-2 text-dark">Client Satisfaction</label>
                        <input type="number" class="form-control border border-danger" name="About_client_sa"
                            value="<%= Aboutdata.About_client_sa %>">
                    </div>

                    <div class="row p-2">
                        <div class="col">
                            <label class="mb-2 text-dark">Specialization</label>
                            <input type="text" class="form-control border border-danger" name="About_Speciali"
                                value="<%= Aboutdata.About_Speciali %>">
                        </div>

                        <div class="col">
                            <label class="mb-2 text-dark">Experience Level</label>
                            <input type="text" class="form-control border border-danger" name="About_Exp_level"
                                value="<%= Aboutdata.About_Exp_level %>">
                        </div>

                        <div class="col">
                            <label class="mb-2 text-dark">Education</label>
                            <input type="text" class="form-control border border-danger" name="About_Education"
                                value="<%= Aboutdata.About_Education %>">
                        </div>

                        <div class="col">
                            <label class="mb-2 text-dark">Languages</label>
                            <input type="text" class="form-control border border-danger" name="About_languages"
                                value="<%= Aboutdata.About_languages %>">
                        </div>



                    </div>


                    <div class="row p-2">
                        <div class="col">
                            <label class="mb-2 text-dark">Happy Clients</label>
                            <input type="number" class="form-control border border-danger" name="About_happy_client"
                                value="<%= Aboutdata.About_happy_client %>">
                        </div>

                        <div class="col">
                            <label class="mb-2 text-dark">Projects</label>
                            <input type="number" class="form-control border border-danger" name="About_projects"
                                value="<%= Aboutdata.About_projects %>">
                        </div>

                        <div class="col">
                            <label class="mb-2 text-dark">Hours Of Support</label>
                            <input type="number" class="form-control border border-danger" name="About_hour_sup"
                                value="<%= Aboutdata.About_hour_sup %>">
                        </div>

                        <div class="col">
                            <label class="mb-2 text-dark">Hard Workers</label>
                            <input type="number" class="form-control border border-danger" name="About_hard_work"
                                value="<%= Aboutdata.About_hard_work %>">
                        </div>

                        <div class="row p-2">
                            <div class="col text-center">
                                <button class="btn btn-success mt-2">Update Home Page</button>
                            </div>
                        </div>

                    </div>
            </form>
        </div>
    </div>


    <div class="row bg-white mt-4">
        <h3 style="font-family:  sans-serif;" class="text-secondary">Skill page </h3>
           <h4 class="text-dark">Front-end Development skill</h4>
        <div class="col-md-12 " style="border-radius: 20px;box-shadow: inset 0px 0px 10px black;">
            <form action="/admin/save_skill_page" method="post" enctype="multipart/form-data">
                <div class="row p-2">
                    <div class="col">
                        <label class="mb-2 pt-2 text-dark">Front skill Title</label>
                        <input type="text"  class="form-control border border-danger" name="skill_f_title" 
                        placeholder="Enter Your Skill Title"   value="">
                    </div>
                    <div class=" col">
                        <label class="mb-2 pt-2 text-dark">technologies</label>
                        <input type="text" class="form-control border border-danger" name="skill_f_technology"
                           placeholder="Enter Your technologies " value="">
                    </div>
                    <div class="col">
                        <label class="mb-2 pt-2 text-dark">progress bar Rang</label>
                        <input type="number" class="form-control border border-danger" name="skill_f_pro_bar_rang"
                           placeholder="Enter Your (Per) Technology" value="">
                    </div>

                    <div class="col">
                        <label class="mb-2 pt-2 text-dark">technologies Decript</label>
                        <input type="text" class="form-control border border-danger" name="skill_f_Decript"
                           placeholder="Enter Your Short Description" value="">
                    </div>

                        <div class="row p-2">
                            <div class="col text-center">
                                <button class="btn btn-success mt-2">front-end save skill Page</button>
                            </div>
                        </div>

                </div>
            </form>
        </div>
    </div>

     <div class="row bg-white mt-4">
        <h3 style="font-family:  sans-serif;" class="text-secondary">Skill page </h3>
           <h4 class="text-dark">Front-end Development skill Update</h4>
        <div class="col-md-12 " style="border-radius: 20px;box-shadow: inset 0px 0px 10px black;">
           
                <div class="row p-2">
                    <div class="col text-center">
                        <label class="mb-2 pt-2 text-dark "><b>skill Edits</b></label>
                        <table class="table table-dark table-hover">
                            <thead>
                                 <tr >
                            <th>Sr No</th>
                            <th>Front skill Title</th>
                            <th>technologies</th>
                            <th>progress bar Rang</th>
                            <th>technologies Decript</th>
                            <th>Action</th>
                          </tr>
                            </thead>
                         
                         <tbody>
                              <tr>
                                       <% for(let i=0; i<result.length ; i++) { %>
                                <td><%= i + 1 %></td>
                                <td><%= result[i].skill_f_title %></td>
                                <td><%= result[i].skill_f_technology %></td>
                                <td><%= result[i].skill_f_pro_bar_rang %></td>
                                <td><%= result[i].skill_f_Decript %></td>
                                <td>
                                    <a href="/admin/skill_edit/<%= result[i].id %>">
                                        <i class="ti ti-edit text-success h3" title="Go To Edit Page"></i>
                                    </a>
                                </td>

                              
                          </tr>
                         </tbody>
                    
                            <% } %>
                        </table>
                    </div>
                   

                </div>
        
        </div>
    </div>


      <div class="row bg-white mt-4">
        <h3 style="font-family:  sans-serif;" class="text-secondary"> back-end Skill page </h3>
           <h4 class="text-dark">Back-end Development skill</h4>
        <div class="col-md-12 " style="border-radius: 20px;box-shadow: inset 0px 0px 10px black;">
            <form action="/admin/save_backend_skill_page" method="post" enctype="multipart/form-data">
                <div class="row p-2">
                    <div class="col">
                        <label class="mb-2 pt-2 text-dark">Backend skill Title</label>
                        <input type="text"  class="form-control border border-danger" name="skill_B_title" 
                        placeholder="Enter Your Skill Title"   value="">
                    </div>
                    <div class=" col">
                        <label class="mb-2 pt-2 text-dark">technologies</label>
                        <input type="text" class="form-control border border-danger" name="skill_B_technology"
                           placeholder="Enter Your technologies " value="">
                    </div>
                    <div class="col">
                        <label class="mb-2 pt-2 text-dark">progress bar Rang</label>
                        <input type="number" class="form-control border border-danger" name="skill_B_pro_bar_rang"
                           placeholder="Enter Your (Per) Technology" value="">
                    </div>

                    <div class="col">
                        <label class="mb-2 pt-2 text-dark">technologies Decript</label>
                        <input type="text" class="form-control border border-danger" name="skill_B_Decript"
                           placeholder="Enter Your Short Description" value="">
                    </div>

                        <div class="row p-2">
                            <div class="col text-center">
                                <button class="btn btn-success mt-2"> back-end save skill Page</button>
                            </div>
                        </div>

                </div>
            </form>
        </div>
    </div>



       <div class="row bg-white mt-4">
        <h3 style="font-family:  sans-serif;" class="text-secondary"> Back-end Skill page </h3>
           <h4 class="text-dark">Back-end Development skill Update</h4>
        <div class="col-md-12 " style="border-radius: 20px;box-shadow: inset 0px 0px 10px black;">
           
                <div class="row p-2">
                    <div class="col text-center">
                        <label class="mb-2 pt-2 text-dark "><b> Back-end skill Edits</b></label>
                        <table class="table table-dark table-hover">
                            <thead>
                                 <tr >
                            <th>Sr No</th>
                            <th>Back-end skill Title</th>
                            <th>technologies</th>
                            <th>progress bar Rang</th>
                            <th>technologies Decript</th>
                            <th>Action</th>
                          </tr>
                            </thead>
                         
                         <tbody>
                              <tr>
                                       <% for(let i=0; i<Back_end.length ; i++) { %>
                                <td><%= i + 1 %></td>
                                <td><%= Back_end[i].skill_B_title %></td>
                                <td><%= Back_end[i].skill_B_technology %></td>
                                <td><%= Back_end[i].skill_B_pro_bar_rang %></td>
                                <td><%= Back_end[i].skill_B_Decript%></td>
                                <td>
                                    <a href="/admin/B_skill_edit/<%= Back_end[i].id %>">
                                        <i class="ti ti-edit text-success h3" title="Go To Edit Page"></i>
                                    </a>
                                </td>

                              
                          </tr>
                         </tbody>
                    
                            <% } %>
                        </table>
                    </div>
                   

                </div>
        
        </div>
    </div>

    <%- include("02.footer.ejs") %>